{
    "root": true,
    "env": {
        "es2021": true
    },
    "extends": [
        "plugin:@typescript-eslint/recommended",
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "valid-jsdoc": ["error", {
            "requireParamType": false,
            "requireReturnType": false,
            "requireReturn": false
        }],
        "require-jsdoc": "error",
        "sort-imports": "error",
        "func-call-spacing": "off",
        "@typescript-eslint/func-call-spacing": ["error"]
    }
}